  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT TOP (1) FactSales.SaleDollars, DimDate.Month, DimDate.Year
FROM     FactSales INNER JOIN
                  DimDate ON FactSales.FullDate = DimDate.FullDate
ORDER BY FactSales.SaleDollars DESC</CommandText>
      </Query>
      <Fields>
        <Field Name="SaleDollars">
          <DataField>SaleDollars</DataField>
